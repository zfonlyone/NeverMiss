{
  "expo": {
    "name": "NeverMiss",
    "slug": "nevermiss",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "nevermiss",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nevermiss.app",
      "jsEngine": "hermes",
      "newArchEnabled": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "package": "com.nevermiss.app",
      "jsEngine": "hermes",
      "newArchEnabled": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ]
    ]
  }
} 