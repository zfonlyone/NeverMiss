{
  "expo": {
    "name": "NeverMiss",
    "slug": "nevermiss",
    "version": "1.1.0",
    "orientation": "portrait",
    "scheme": "nevermiss",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nevermiss.app"
    },
    "android": {
      "package": "com.nevermiss.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
} 